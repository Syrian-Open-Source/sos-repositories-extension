{
    "name": "Syrian Open Source",
    "version": "1.0.0",
    "description": "A very basic extension to show a syrian open source repositories.",
    "permissions": [
        "webRequest"
    ],
    "icons": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/logo.png"
    },
    "manifest_version": 2
}